{
	"manifest_version": 2,
	"name": "Chrome插件demo",
	"version": "1.0",
	"description": "",
	"author": "",
	"icons":
	{
		"64": "img/icon-64.png"
	},
	"browser_action": 
	{
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
		  "js": ["js/content-script.js"],
		  "matches": [
			"http://*/*",
			"https://*/*"
		  ],
		  "run_at": "document_start"
		}
	],
	"permissions": [
		"tabs"
	]
}